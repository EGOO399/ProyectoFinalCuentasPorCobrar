@model IEnumerable<CuentasFinal.Models.ESTADOCREDITO>

@{
    ViewBag.Title = "Index";
}

<img src="~/Scripts/logo.jpg" width="200">

<p>
    @Html.ActionLink("Nuevo ", "Create")
</p>
<table class="table table-bordered ">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdEstado }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdEstado }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdEstado })
        </td>
    </tr>
}

</table>
